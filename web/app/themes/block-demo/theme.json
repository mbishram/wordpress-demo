{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"border": {
			"radiusSizes": [
				{
					"name": "Small",
					"size": "8px",
					"slug": "small"
				}
			]
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"palette": [
				{
					"color": "oklch(62.3% 0.214 259.815)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "oklch(70.7% 0.022 261.325)",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "48rem",
			"wideSize": "80rem"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "2xs",
					"size": "0.25rem",
					"slug": "2xs"
				},
				{
					"name": "xs",
					"size": "0.5rem",
					"slug": "xs"
				},
				{
					"name": "sm",
					"size": "1rem",
					"slug": "sm"
				},
				{
					"name": "md",
					"size": "1.5rem",
					"slug": "md"
				},
				{
					"name": "lg",
					"size": "3rem",
					"slug": "lg"
				},
				{
					"name": "xl",
					"size": "4rem",
					"slug": "xl"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "ui-sans-serif, system-ui, sans-serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol, Noto Color Emoji",
					"name": "Sans",
					"slug": "sans"
				},
				{
					"fontFamily": "ui-serif, Georgia, Cambria, Times New Roman, Times, serif",
					"name": "Serif",
					"slug": "serif"
				},
				{
					"fontFamily": "ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, Liberation Mono, Courier New, monospace",
					"name": "Mono",
					"slug": "mono"
				}
			],
			"fontSizes": [
				{
					"name": "xs",
					"size": "0.625rem",
					"slug": "extra-small"
				},
				{
					"name": "sm",
					"size": "0.825rem",
					"slug": "small"
				},
				{
					"name": "md",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"name": "lg",
					"size": "1.25rem",
					"slug": "large"
				},
				{
					"name": "xl",
					"size": "2.5rem",
					"slug": "extra-large"
				},
				{
					"name": "2xl",
					"size": "4rem",
					"slug": "extra-extra-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"css": "list-style: none;",
				"spacing": {
					"padding": {
						"left": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "var(--wp--preset--border-radius--small)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)"
				}
			},
			"core/categories": {
				"css": "list-style: none;",
				"spacing": {
					"padding": {
						"left": "0"
					}
				}
			},
			"core/code": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mono)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"core/image": {
				"border": {
					"radius": "var(--wp--preset--border-radius--small)"
				}
			},
			"core/latest-comments": {
				"css": "& li { margin-bottom: var(--wp--preset--spacing--xs); }",
				"spacing": {
					"padding": {
						"left": "0"
					}
				}
			},
			"core/latest-posts": {
				"css": "& li { margin-bottom: var(--wp--preset--spacing--xs); }"
			},
			"core/list": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"top": "0"
					},
					"padding": {
						"bottom": "0",
						"left": "var(--wp--preset--spacing--sm)",
						"right": "0",
						"top": "0"
					}
				}
			},
			"core/paragraph": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--black)"
						}
					}
				}
			},
			"core/post-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--extra-large)"
				}
			},
			"core/quote": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"left": {
						"color": "var(--wp--preset--color--secondary)",
						"style": "solid",
						"width": "4px"
					},
					"right": {
						"style": "none",
						"width": "0px"
					},
					"top": {
						"style": "none",
						"width": "0px"
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					},
					"padding": {
						"bottom": "var(--wp--preset--spacing--md)",
						"left": "var(--wp--preset--spacing--md)",
						"right": "0",
						"top": "var(--wp--preset--spacing--md)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--extra-large)"
				}
			},
			"core/search": {
				"elements": {
					"button": {
						"border": {
							"radius": {
								"ref": "styles.blocks.core/button.border.radius"
							}
						},
						"color": {
							"background": {
								"ref": "styles.blocks.core/button.color.background"
							}
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--secondary)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-extra-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"heading": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "500",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--black)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			},
			"textInput": {
				"border": {
					"radius": "var(--wp--preset--border-radius--small)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--md)",
			"padding": {
				"left": "var(--wp--preset--spacing--md)",
				"right": "var(--wp--preset--spacing--md)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans)",
			"fontSize": "var(--wp--preset--font-size--md)",
			"letterSpacing": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "query-loop",
			"title": "Query Loop"
		}
	]
}