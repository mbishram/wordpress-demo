{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"palette": [
				{
					"color": "oklch(62.3% 0.214 259.815)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "oklch(70.7% 0.022 261.325)",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#FFF",
					"name": "White",
					"slug": "white"
				}
			]
		},
		"layout": {
			"contentSize": "48rem",
			"wideSize": "96rem"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "2xs",
					"size": "0.25rem",
					"slug": "2xs"
				},
				{
					"name": "xs",
					"size": "0.5rem",
					"slug": "xs"
				},
				{
					"name": "sm",
					"size": "1rem",
					"slug": "sm"
				},
				{
					"name": "md",
					"size": "1.5rem",
					"slug": "md"
				},
				{
					"name": "lg",
					"size": "3rem",
					"slug": "lg"
				},
				{
					"name": "xl",
					"size": "4rem",
					"slug": "xl"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "ui-sans-serif, system-ui, sans-serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol, Noto Color Emoji",
					"name": "Sans",
					"slug": "sans"
				},
				{
					"fontFamily": "ui-serif, Georgia, Cambria, Times New Roman, Times, serif",
					"name": "Serif",
					"slug": "serif"
				},
				{
					"fontFamily": "ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, Liberation Mono, Courier New, monospace",
					"name": "Mono",
					"slug": "mono"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--black)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--md)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"letterSpacing": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "query-loop",
			"title": "Query Loop"
		}
	]
}